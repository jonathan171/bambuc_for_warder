
{% block body %}
    <h1>Envio {{ envios_nacionale.numero}}</h1>

    <table class="table">
        <tbody>
           
            <tr>
                <th>Dirección Destino</th>
                <td>{{ envios_nacionale.direccionDestino }}</td>
            </tr>
            <tr>
                <th>Télefono Destinatario</th>
                <td>{{ envios_nacionale.telefonoDestinatario }}</td>
            </tr>
            <tr>
                <th>Peso</th>
                <td>{{ envios_nacionale.peso }}</td>
            </tr>
            <tr>
                <th>Descripción</th>
                <td>{{ envios_nacionale.descripcion }}</td>
            </tr>
            <tr>
                <th>Seguro</th>
                <td>{{ envios_nacionale.seguro }}</td>
            </tr>
            <tr>
                <th>Valor Total</th>
                <td>{{ envios_nacionale.valorTotal }}</td>
            </tr>
            <tr>
                <th>Forma Pago</th>
                <td>{{ envios_nacionale.formaPago }}</td>
            </tr>
           
        </tbody>
    </table>

   
{% endblock %}
