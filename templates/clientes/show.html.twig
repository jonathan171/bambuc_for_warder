{% extends 'base.html.twig' %}

{% block title %}Clientes{% endblock %}

{% block body %}
    <h1>Clientes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cliente.id }}</td>
            </tr>
            <tr>
                <th>RazonSocial</th>
                <td>{{ cliente.razonSocial }}</td>
            </tr>
            <tr>
                <th>Nit</th>
                <td>{{ cliente.nit }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ cliente.direccion }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ cliente.telefono }}</td>
            </tr>
            <tr>
                <th>Correo</th>
                <td>{{ cliente.correo }}</td>
            </tr>
            <tr>
                <th>TipoReceptor</th>
                <td>{{ cliente.tipoReceptor ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>TipoDocumento</th>
                <td>{{ cliente.tipoDocumento }}</td>
            </tr>
            <tr>
                <th>TipoRegimen</th>
                <td>{{ cliente.tipoRegimen }}</td>
            </tr>
            <tr>
                <th>Nombres</th>
                <td>{{ cliente.nombres }}</td>
            </tr>
            <tr>
                <th>Apellidos</th>
                <td>{{ cliente.apellidos }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_clientes_index') }}">back to list</a>

    <a href="{{ path('app_clientes_edit', {'id': cliente.id}) }}">edit</a>

    {{ include('clientes/_delete_form.html.twig') }}
{% endblock %}
