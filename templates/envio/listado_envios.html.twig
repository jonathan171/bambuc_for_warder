<form id="itemsForm"
    action="{{ path('app_factura_facturar_envios')}}"
    method="post"
    enctype="multipart/form-data">
     
        

    <table class="table customize-table mb-0 v-middle">
      <thead class="table-light">
        <tr>
          <th>Escoger</th>
          <th>Codigo</th>
          <th>Fecha</th>
          <th>Envia</th>
        </tr>
      </thead>
      <tbody>
      
          {% for envio in envios %}
        <tr>
          <td><input name="envId[]" id="checkBox" value="{{envio.id}}" type="checkbox"></td>
          <td>{{envio.numeroEnvio}}</td>
          <td>{{envio.fechaEnvio|date('Y-m-d')}}</td>        
          <td>{{envio.quienEnvia}}</td>
         

        </tr>
       {% endfor %}
        
        <tr>
            <td colspan="2"><input type="submit" class="btn btn-success" value="Facturar" /></td>
            <td colspan="2"></td>
        </tr>
        
      </tbody>
    </table>
    <input type="hidden" value="{{factura_id}}" name="factura_id" id="factura_id" >
</form>   