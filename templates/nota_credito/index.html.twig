{% block title %}NotaCredito index{% endblock %}

{% block body %}
    <h1>NotaCredito index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>Hora</th>
                <th>NumeroNota</th>
                <th>Subtotal</th>
                <th>Total</th>
                <th>TotalIva</th>
                <th>ReteFuente</th>
                <th>TotalReteFuenteG</th>
                <th>Observaciones</th>
                <th>Tipo</th>
                <th>ConceptoDebito</th>
                <th>ConceptoCredito</th>
                <th>RespuestaDian</th>
                <th>Cufe</th>
                <th>CuerpoJsonf</th>
                <th>CuerpoJsonc</th>
                <th>RespuestaCorreo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for nota_credito in nota_creditos %}
            <tr>
                <td>{{ nota_credito.id }}</td>
                <td>{{ nota_credito.fecha ? nota_credito.fecha|date('Y-m-d') : '' }}</td>
                <td>{{ nota_credito.hora ? nota_credito.hora|date('H:i:s') : '' }}</td>
                <td>{{ nota_credito.numeroNota }}</td>
                <td>{{ nota_credito.subtotal }}</td>
                <td>{{ nota_credito.total }}</td>
                <td>{{ nota_credito.totalIva }}</td>
                <td>{{ nota_credito.reteFuente }}</td>
                <td>{{ nota_credito.totalReteFuenteG }}</td>
                <td>{{ nota_credito.observaciones }}</td>
                <td>{{ nota_credito.tipo }}</td>
                <td>{{ nota_credito.conceptoDebito }}</td>
                <td>{{ nota_credito.conceptoCredito }}</td>
                <td>{{ nota_credito.respuestaDian }}</td>
                <td>{{ nota_credito.cufe }}</td>
                <td>{{ nota_credito.cuerpoJsonf }}</td>
                <td>{{ nota_credito.cuerpoJsonc }}</td>
                <td>{{ nota_credito.respuestaCorreo }}</td>
                <td>
                    <a href="{{ path('app_nota_credito_show', {'id': nota_credito.id}) }}">show</a>
                    <a href="{{ path('app_nota_credito_edit', {'id': nota_credito.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="19">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_nota_credito_new') }}">Create new</a>
{% endblock %}
